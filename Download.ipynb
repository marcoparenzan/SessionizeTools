      "source": [
        "var httpClient = new System.Net.Http.HttpClient();\n",
        "for(var i = 0; i<49; i++)\n",
        "{\n",
        "    var bytes = await httpClient.GetByteArrayAsync($\"https:///api/slide?idx={i}\");\n",
        "    await System.IO.File.WriteAllBytesAsync($\"Slide-{i:00}\", bytes);\n",
        "}"
      ],
